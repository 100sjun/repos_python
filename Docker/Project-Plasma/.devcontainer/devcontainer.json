{
	"name": "Python Dev (pip)",
	"dockerComposeFile": "../docker-compose.yml",
	"service": "dev",
	"workspaceFolder": "/workspace",
	"customizations": {
	  "vscode": {
		"settings": {
		  "terminal.integrated.defaultProfile.linux": "bash",
		  "python.defaultInterpreterPath": "/usr/local/bin/python"
		},
		"extensions": [
		  "ms-python.python",
		  "ms-toolsai.jupyter"
		]
	  }
	},
	"remoteUser": "root"
  }